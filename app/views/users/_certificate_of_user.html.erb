<div class="col-lg-12">
  <div class="main-box clearfix">
    <header class="main-box-header clearfix">
      <h2>Certificates</h2>
    </header>
    <div class="main-box-body clearfix">
      <ul class="widget-users row">
        <% object.each do |certificate_user| %>
        <li class="col-md-6" style="list-style: none;">
          <div class="img">
            <%= image_tag certificate_user.certification.image.url %>
          </div>
          <div class="details">
            <div class="name">
              <a href="#"><%= certificate_user.certification.name %></a>
            </div>
            <div class="time online">
              <i class="fa fa-check-circle"></i> Certification
            </div>
          </div>
        </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>